						"exec": [
							"pm.test(\"Status is 200\", () => {",
							"pm.response.to.have.status(200)",
							"})",
							"",
							"const j = pm.response.json()",
							"",
							"pm.test(\"Data has required fields\", () => {",
							"pm.expect(j.data).to.have.property(\"id\")",
							"pm.expect(j.data).to.have.property(\"email\")",
							"pm.expect(j.data).to.have.property(\"first_name\")",
							"pm.expect(j.data).to.have.property(\"last_name\")",
							"pm.expect(j.data.id).to.eql(2)",
							"})",
							"",
							"pm.test(\"Content Type is json\", () => {",
							"pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\")",
							"})"
